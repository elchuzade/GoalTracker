    "url": "https://github.com/elchuzade/GoalTracker/issues"
  },
  "homepage": "https://github.com/elchuzade/GoalTracker#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "validator": "^13.1.1"
  }
